pipeline {
  agent any 
stages {
  stage('Clean'){
  steps{
    sh 'mvn clean'
     }
  }
   stage('install'){
       steps{
    sh 'mvn install'
           }
      }  
  stage('publish'){
    steps{
    nexusPublisher nexusInstanceId: 'admin', nexusRepositoryId: 'maven-snapshots', packages: [[$class: 'MavenPackage', mavenAssetList: [[classifier: '', extension: '', filePath: '/var/lib/jenkins/workspace/fabric-sdk-java/target/fabric-sdk-java-2.0.0-SNAPSHOT.jar']], mavenCoordinate: [artifactId: 'fabric-sdk-java', groupId: 'org.hyperledger.fabric-sdk-java', packaging: 'jar', version: '2.0.0-SNAPSHOT']]]
    }
       }
     }
   }
